interface types {
    record idp2p-error-param {
        key: string,
        value: string
    }

    record idp2p-error {
        code: string,
        message: string,
        details: list<idp2p-error-param>
    }
    
    record id-claim-event {
        id: string,
        created-at: string,
        payload: list<u8>,
    }

    record id-signer {
        id: string,
        public-key: list<u8>,
        valid-from: string,
        valid-until: option<string>
    }

    record id-delegator {
        id: string,
        scope: list<string>
    }

    record id-state {
        id: string,
        aka: list<u8>,
        prior-id: option<string>,
        event-id: string,
        event-timestamp: string,
        threshold: u8,
        next-threshold: u8,
        signers: list<id-signer>,
        current-signers: list<string>,
        next-signers: list<string>,
        delegators: list<id-delegator>,
        claim-events: list<id-claim-event>
    }

    record id-event-proof {
        key-id: string,
        created-at: string,
        signature: list<u8>
    }

    record id-event-receipt {
        id: string,
        version: string,
        created-at: string,
        payload: list<u8>,
        proofs: list<id-event-proof>,
        delegated-proofs: list<id-proof>
    }

    
    record id-proof {
        id: string,
        purpose: string,
        version: string,
        key-id: string,
        created-at: string,
        signature: list<u8>
    }

    record did {
        id: string,
        inception: id-event-receipt,
        events: list<id-event-receipt>
    }
}