interface types {
   record id-envelope {
      method: u16,
      // 0..
      major: u16,
      // 1..
      minor: u16,
      // 32 bytes hash
      patch: list<u8>,
      // content
      body: list<u8>,
   }

   record persisted-id-event {
      id: string,
      payload: id-envelope,
      signatures: list<list<u8>>,
      proofs: list<id-envelope>
   }

   record persisted-id {
      id: string,
      inception: persisted-id-event,
      events: list<persisted-id-event>
   }
}
